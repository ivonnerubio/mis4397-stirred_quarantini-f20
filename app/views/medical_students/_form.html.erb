
<%= simple_form_for(@medical_student) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :education %>
    <%= f.input :background %>
    <%= f.input :award %>
    <%= f.input :certification %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
